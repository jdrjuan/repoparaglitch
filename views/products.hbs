<section>
    <h1>Productos</h1>
    <div class="table-container">
        <table class="products-table">
            <caption>Listado de productos</caption>
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Nombre</th>
                    <th>Marca</th>
                    <th>Precio</th>
                    <th>Stock</th>
                    <th>Categor√≠a</th>
                </tr>
            </thead>
            <tbody>
                {{#each products}}
                    <tr>
                        <td>
                            <a href="/products/{{this.id}}" title="Ver detalles del producto">
                                {{this.id}}
                            </a>
                        </td>
                        <td>{{this.name}}</td>
                        <td>{{this.brand}}</td>
                        <td>${{this.price}}</td>
                        <td>{{this.stock}}</td>
                        <td>{{this.category}}</td>
                    </tr>
                {{/each}}
            </tbody>
            <tfoot>
                <tr>
                    <td colspan="7">Se encontraron {{products.length}} productos</td>
                </tr>
            </tfoot>
        </table>
    </div>
</section>

